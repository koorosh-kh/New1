<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sprint_home_page"> 

<listeners>

<listener class-name="Com.Sprint.Sprint_Home.App"></listener>

</listeners> 

<test name="SPRINT">

<parameter name="url" value="https://www.sprint.com/"></parameter>

<parameter name="browser" value="chrome"></parameter>

<classes>
<class name="RunSP.Search_SP"></class>
</classes>
</test>
<test name="SPRINT!">

<parameter name="url" value="https://www.sprint.com/"></parameter>

<parameter name="browser" value="firefox"></parameter>

<classes>
<class name="RunSP.Search_SP"></class>

</classes>
</test>
</suite>